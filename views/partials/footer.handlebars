<script src="plugin/jquery_mlkeyboard/jquery.ml-keyboard.js"></script>

<script type="text/javascript">
    $(document).ready(function () {

        $('input#keyboardText').mlKeyboard({
            layout: 'en_US',
            active_shift: false,
            active_caps: false,
            trigger: '#keyforinput',
            is_hidden: true,
        });

        $("input[name='optradio']").click(function(){
            var val = $(this).val();
            $("#keyboardText").val("");
            $("#keyboardText").val(val+" ");
        });

    });
</script>
